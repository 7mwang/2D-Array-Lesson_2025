    "// Display the full list\n",
    "console.log(\"This is your list: \", aList);\n",
    "\n",
    "// Delete the second element (index 1)\n",
    "aList.splice(1, 1);\n",
    "\n",
    "// Display the list after deletion\n",
    "console.log(\"This is your new list: \", aList);\n"
